func _hideAllNodes():
	for obj_node in Global.objNodes:
		obj_node.hide()


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	if Global.paused == false:
		#here the different objects should be shown one by one
		Global.delta_counter += delta
		if Global.delta_counter > 5:
	#		print('delta counter > 2')
			Global.delta_counter = 0
			
			if Global.node_counter == 0:
	#			print('Global.objNodes[Global.node_counter is ', Global.objNodes[Global.node_counter])
				Global.objNodes[Global.node_counter].show()
				Global.objNodes[-1].hide()
			else:
				Global.objNodes[Global.node_counter].show()
				Global.objNodes[(Global.node_counter-1)].hide()
				
			
	#		print(Global.node_counter)
			Global.node_counter += 1
			
			
			if Global.node_counter >= len(Global.objNodes):
				Global.node_counter = 0

func _input(event):
	if event.is_action_pressed("Menu"):
		get_tree().change_scene_to_file("res://Main_menu.tscn")
	
	if event.is_action_pressed("pause"):
		if Global.paused == true:
			_hideAllNodes()
			$"../CanvasLayer/Pause".hide()
			Global.paused = false
		else:
			$"../CanvasLayer/Pause".show()
			Global.paused = true
